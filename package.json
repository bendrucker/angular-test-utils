{
  "name": "ng-test-utils",
  "version": "0.0.6",
  "description": "angular test utilities",
  "main": "index.js",
  "scripts": {
    "lint": "jshint src/** test/**",
    "test": "npm run lint && npm run cover",
    "test-recast": "mocha mocha-globals.js test/*-test.js test/**/*-test.js",
    "test-esprima": "NG_UTILS_PARSER=ESPRIMA mocha mocha-globals.js test/*-test.js test/**/*-test.js",
    "test-esprima": "NG_UTILS_PARSER=ACORN mocha mocha-globals.js test/*-test.js test/**/*-test.js",
    "watch": "mocha -w --growl mocha-globals.js test/*-test.js  test/**/*-test.js",
    "cover": "rm -rf coverage && npm run cover-recast && npm run cover-esprima && node combine-coverage",
    "cover-recast": "istanbul cover --report none --dir ./coverage/nohack _mocha -- mocha-globals.js test/*-test.js  test/**/*-test.js",
    "cover-esprima": "NG_UTILS_PARSER=ESPRIMA istanbul cover --report none --dir ./coverage/hack _mocha -- mocha-globals.js test/*-test.js  test/**/*-test.js",
    "cover-acorn": "NG_UTILS_PARSER=ACORN istanbul cover --report none --dir ./coverage/hack _mocha -- mocha-globals.js test/*-test.js  test/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamestalmage/angular-test-utils.git"
  },
  "keywords": [
    "angular",
    "testing"
  ],
  "author": "James Talmage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamestalmage/angular-test-utils/issues"
  },
  "homepage": "https://github.com/jamestalmage/angular-test-utils",
  "dependencies": {
    "recast": "^0.10.1",
    "convert-source-map": "git://github.com/jamestalmage/convert-source-map#4397e24b9a41b19b964ea0a4b6eb0858d6a3450f"
  },
  "devDependencies": {
    "acorn": "^0.12.0",
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "esprima-fb": "^12001.1.0-dev-harmony-fb",
    "glob": "^4.4.0",
    "istanbul": "^0.3.6",
    "jshint": "^2.6.3",
    "mocha": "^2.1.0",
    "proxyquire": "^1.3.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0"
  }
}
