{
  "name": "ng-test-utils",
  "version": "0.0.1",
  "description": "angular test utilities",
  "main": "index.js",
  "scripts": {
    "lint": "jshint src/** test/**",
    "test": "npm run cover && npm run lint",
    "test-nohack": "mocha mocha-globals.js test/**/*-test.js",
    "test-hack": "RECAST_COMMENT_HACK=true mocha mocha-globals.js test/**/*-test.js",
    "watch": "mocha -w --growl mocha-globals.js test/**/*-test.js",
    "cover": "rm -rf coverage && npm run cover-nohack && npm run cover-hack && node combine-coverage",
    "cover-nohack": "istanbul cover --report none --dir ./coverage/nohack _mocha -- mocha-globals.js  test/**/*-test.js",
    "cover-hack": "RECAST_COMMENT_HACK=true istanbul cover --report none --dir ./coverage/hack _mocha -- mocha-globals.js  test/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamestalmage/angular-test-utils.git"
  },
  "keywords": [
    "angular",
    "testing"
  ],
  "author": "James Talmage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamestalmage/angular-test-utils/issues"
  },
  "homepage": "https://github.com/jamestalmage/angular-test-utils",
  "dependencies": {
    "ast-types": "^0.6.15",
    "merge": "^1.2.0",
    "recast": "^0.10.1",
    "source-map": "^0.4.0",
    "through2": "^0.6.3",
    "transformify": "^0.1.2"
  },
  "devDependencies": {
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "esprima-fb": "^12001.1.0-dev-harmony-fb",
    "glob": "^4.4.0",
    "istanbul": "^0.3.6",
    "jshint": "^2.6.3",
    "mocha": "^2.1.0",
    "proxyquire": "^1.3.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0"
  }
}
