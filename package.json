{
  "name": "ng-test-utils",
  "version": "0.0.12",
  "description": "angular test utilities",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamestalmage/angular-test-utils.git"
  },
  "keywords": [
    "angular",
    "testing"
  ],
  "author": "James Talmage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamestalmage/angular-test-utils/issues"
  },
  "homepage": "https://github.com/jamestalmage/angular-test-utils",
  "dependencies": {
    "convert-source-map": "git://github.com/jamestalmage/convert-source-map#4397e24b9a41b19b964ea0a4b6eb0858d6a3450f",
    "merge": "^1.2.0",
    "recast": "^0.10.1"
  },
  "devDependencies": {
    "acorn": "^0.12.0",
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "esprima-fb": "^12001.1.0-dev-harmony-fb",
    "glob": "^4.4.0",
    "gulp": "^3.8.11",
    "gulp-clean": "^0.3.1",
    "gulp-istanbul": "^0.6.0",
    "gulp-jscs": "^1.4.0",
    "gulp-jshint": "^1.9.2",
    "gulp-mocha": "^2.0.0",
    "gulp-util": "^3.0.4",
    "istanbul": "^0.3.6",
    "jscs": "^1.11.3",
    "jshint": "^2.6.3",
    "mocha": "^2.1.0",
    "proxyquire": "^1.3.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0"
  },
  "scripts": {
    "test": "npm run lint && npm run cover && npm run check-style",
    "test-all": "npm test && gulp test-plugin",
    "cover": "rm -rf coverage && npm run cover-recast && npm run cover-esprima && npm run cover-acorn && node combine-coverage",
    "watch": "mocha -w --growl mocha-globals.js test/*-test.js  test/**/*-test.js",
    "lint": "gulp lint",
    "check-style": "gulp check-style",
    "cover-recast": "NG_UTILS_PARSER=recast gulp cover",
    "cover-esprima": "NG_UTILS_PARSER=esprima gulp cover",
    "cover-acorn": "NG_UTILS_PARSER=acorn gulp cover"
  }
}
